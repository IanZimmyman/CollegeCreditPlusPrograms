Option Strict On
Option Explicit On
Module Module1
    'Main should display 5 facts about yourself to the screen. 
    Sub Main()
        'use WriteLine to display a fact about yourself
        Console.WriteLine("I have never broken a bone.")
        Console.WriteLine("My favorite food is pizza.")
        Console.WriteLine("I have a YouTube channel.")
        Console.WriteLine("I am currently in development of a game with Tyler and Bailey.")
        Console.WriteLine("I was watching dinosaur documentaries when I was three.")
        Console.ReadLine()
        'repeat process for four more facts
    End Sub

End Module
